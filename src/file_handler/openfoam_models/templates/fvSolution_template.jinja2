{# Plantilla Jinja2 para el archivo 'fvSolution' de OpenFOAM #}

{% if params.solver_selected == 'damBreak' %}

    solvers
        {
            "alpha.water.*"
            {
                nAlphaCorr      2;
                nAlphaSubCycles 1;
                cAlpha          1;

                MULESCorr       yes;
                nLimiterIter    5;

                solver          smoothSolver;
                smoother        symGaussSeidel;
                tolerance       {{ params.alpha_water_tolerance }};
                relTol          {{ params.alpha_water_relTol }};
            }

            "pcorr.*"
            {
                solver          PCG;
                preconditioner  DIC;
                tolerance       1e-5;
                relTol          0;
            }

            p_rgh
            {
                solver          PCG;
                preconditioner  DIC;
                tolerance       {{ params.p_rgh_tolerance }};
                relTol          {{ params.p_rgh_relTol }};
            }

            p_rghFinal
            {
                $p_rgh;
                relTol          0;
            }

            U
            {
                solver          smoothSolver;
                smoother        symGaussSeidel;
                tolerance       {{ params.U_tolerance }};
                relTol          {{ params.U_relTol }};
            }
        }

        PIMPLE
        {
            momentumPredictor   no;
            nOuterCorrectors    {{ params.nOuterCorrectors }};
            nCorrectors         {{ params.nCorrectors }};
            nNonOrthogonalCorrectors {{ params.nNonOrthogonalCorrectors }};
        }

        relaxationFactors
        {
            equations
            {
                ".*" 1;
            }
        }
{% endif %}

{%if params.solver_selected == 'waterChannelOpenFoam'%}
solvers
{
    "alpha.water.*"
    {
        nAlphaCorr      {{ params.nAlphaCorr }};
        nAlphaSubCycles {{ params.nAlphaSubCycles }};
        cAlpha          {{ params.cAlpha }};

        MULESCorr       yes;
        nLimiterIter    {{ params.nLimiterIter }};

        solver          smoothSolver;
        smoother        symGaussSeidel;
        tolerance       {{ params.alpha_water_tolerance }};
        relTol          {{ params.alpha_water_relTol }};
    }

    "pcorr.*"
    {
        solver          PCG;
        preconditioner
        {
            preconditioner  GAMG;
            tolerance       1e-5;
            relTol          0;
            smoother        GaussSeidel;
        }
        tolerance       1e-5;
        relTol          0;
        maxIter         50;
    }

    p_rgh
    {
        solver           GAMG;
        tolerance        {{ params.p_rgh_tolerance }};
        relTol           {{ params.p_rgh_relTol }};
        smoother         GaussSeidel;
        maxIter          {{ params.p_rgh_maxIter }};
    };

    p_rghFinal
    {
        $p_rgh;
        tolerance       {{ params.p_rgh_tolerance }};
        relTol          0;
    }

    "(U|k|omega|s).*"
    {
        solver          smoothSolver;
        smoother        symGaussSeidel;
        nSweeps         {{ params.U_k_omega_s_nSweeps }};
        tolerance       {{ params.U_k_omega_s_tolerance }};
        relTol          {{ params.U_k_omega_s_relTol }};
    };
}

PIMPLE
{
    momentumPredictor no;
    nCorrectors     {{ params.nCorrectors }};
    nNonOrthogonalCorrectors {{ params.nNonOrthogonalCorrectors }};
}

relaxationFactors
{
    equations
    {
        ".*" 1;
    }
}

{%endif%}


{%if params.solver_selected == '2DChannelSedFoam'%}
solvers
{
    p_rbgh
    {
        solver          GAMG;
        tolerance       {{ params.p_rbgh_tolerance }};
        relTol          {{ params.p_rbgh_relTol }};
        smoother        DIC;
        nPreSweeps      {{ params.p_rbgh_nPreSweeps }};
        nPostSweeps     {{ params.p_rbgh_nPostSweeps }};
        nFinestSweeps   {{ params.p_rbgh_nFinestSweeps }};
        cacheAgglomeration true;
        nCellsInCoarsestLevel 10;
        agglomerator    faceAreaPair;
        mergeLevels     1;
    }

    p_rbghFinal
    {
        $p_rbgh;
        tolerance       {{ params.p_rbgh_tolerance }};
        relTol          0;
    }

    "(k.b|epsilon.b|omega.b|pa_new_value|alphaPlastic)"
    {
        solver          PBiCG;
        preconditioner  DILU;
        tolerance       {{ params.k_epsilon_omega_pa_alpha_tolerance }};
        relTol          {{ params.k_epsilon_omega_pa_alpha_relTol }};
    }

    "(k.bFinal|epsilon.bFinal|omega.bFinal|paFinal|alphaPlasticFinal)"
    {
        solver          PBiCG;
        preconditioner  DILU;
        tolerance       {{ params.k_epsilon_omega_pa_alpha_tolerance }};
        relTol          0;
    }

    "U.a|U.aFinal"
    {
        solver          PBiCG;
        preconditioner  DILU;
        tolerance       1e-6;
        relTol          0;
    }

    "U.b|U.bFinal"
    {
        solver          PBiCG;
        preconditioner  DILU;
        tolerance       1e-6;
        relTol          0;
    }

    alpha.a
    {
        solver          PBiCG;
        preconditioner  DILU;
        tolerance       1e-15;
        relTol          0;
    }
    alpha.aFinal
    {
        solver          PBiCG;
        preconditioner  DILU;
        tolerance       1e-15;
        relTol          0;
    }
    Theta
    {
        solver          PBiCG;
        preconditioner  DILU;
        tolerance       1e-10;
        relTol          0;
    }
    ThetaFinal
    {
        solver          PBiCG;
        preconditioner  DILU;
        tolerance       1e-10;
        relTol          0;
    }
}


relaxationFactors
{
    fields
    {
        pa              1.;
    }
}

PIMPLE
{
    nCorrectors     {{ params.nCorrectors }};
    nNonOrthogonalCorrectors {{ params.nNonOrthogonalCorrectors }};
    correctAlpha    0;
    nAlphaCorr      {{ params.nAlphaCorr }};
    
}

{%endif%}


