{# Plantilla Jinja2 para el archivo 'transportProperties' de OpenFOAM #}

{% if params.solver_selected == 'damBreak' %}
phases      (water air);

water
{
    transportModel  {{params.water_transportModel}};
    nu              {{params.water_nu}};
    rho             {{params.water_rho}};
}

air
{
    transportModel  {{params.air_transportModel}};
    nu              {{params.air_nu}};
    rho             {{params.air_rho}};
}

sigma       {{ params.sigma }};
{% endif %}

{% if params.solver_selected == '2DChannel' %}

// * * * * * * * * * * * * sediment properties * * * * * * * * * * * * //
phasea
{
    rho             rho [ 1 -3 0 0 0 ] 1;
    nu              nu [ 0 2 -1 0 0 ] 0e0;
    d               d [ 0 1 0 0 0 0 0 ] {{params.da}};
    sF              sF   [ 0 0  0 0 0 0 0 ] 1;
    hExp            hExp [ 0 0  0 0 0 0 0 ] 2.65;  // hindrance exponent for drag: beta^(-hExp) (2.65 by default)
}

phaseb
{
    rho             rho [ 1 -3 0 0 0 ] 1;
    nu              nu [ 0 2 -1 0 0 ] 7.2727e-5;
    d               d [ 0 1 0 0 0 0 0 ] {{params.db}};
    sF              sF   [ 0 0  0 0 0 0 0 ] 1;
    hExp            hExp [ 0 0  0 0 0 0 0 ] 2.65;
}
//*********************************************************************** //
transportModel  Newtonian;

nu              nu [ 0 2 -1 0 0 0 0 ] 7.2727e-5;

nuMax           nuMax [0 2 -1 0 0 0 0] 1e-1;      // viscosity limiter for the Frictional model (required for stability)

alphaSmall      alphaSmall [ 0 0 0 0 0 0 0 ] 1e-6;  // minimum volume fraction (phase a) for division by alpha 

// ************************************************************************* //

{% endif %}



