{# Plantilla Jinja2 para el archivo 'transportProperties' de OpenFOAM #}

{% if params.solver_selected == 'interFoam' %}
phases      (water air);

water
{
    transportModel  {{params.water_transportModel}};
    nu              {{params.water_nu}};
    rho             {{params.water_rho}};
}

air
{
    transportModel  {{params.air_transportModel}};
    nu              {{params.air_nu}};
    rho             {{params.air_rho}};
}

sigma       {{ params.sigma }};
{% endif %}

{% if params.solver_selected == 'sedFoam' %}

// * * * * * * * * * * * * sediment properties * * * * * * * * * * * * //
phasea
{
    rho             rho [ 1 -3 0 0 0 ] {{params.rhoa}};
    nu              nu [ 0 2 -1 0 0 ] {{params.nua}};
    d               d [ 0 1 0 0 0 0 0 ] {{params.da}};
    sF              sF   [ 0 0  0 0 0 0 0 ] {{params.sFa}};
    hExp            hExp [ 0 0  0 0 0 0 0 ] {{params.hExpa}};  // hindrance exponent for drag: beta^(-hExp) (2.65 by default)
}

phaseb
{
    rho             rho [ 1 -3 0 0 0 ] {{params.rhob}};
    nu              nu [ 0 2 -1 0 0 ] {{params.nub}};
    d               d [ 0 1 0 0 0 0 0 ] {{params.db}};
    sF              sF   [ 0 0  0 0 0 0 0 ] {{params.sFb}};
    hExp            hExp [ 0 0  0 0 0 0 0 ] {{params.hExpb}};
}
//*********************************************************************** //
{% endif %}

{%if customContent is not none%}
{{customContent}}
{%endif%}

