{# Plantilla Jinja2 para el archivo 'k' de OpenFOAM #}

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform {{ internalField }};

boundaryField
{
    {% for patch in boundaryField %}
    {{ patch.patchName }}
    {
        type            {{ patch.type }};
        {% if patch.value is defined %}
        value           uniform {{ patch.value }};
        {% endif %}
    }
    {% endfor %}
}
